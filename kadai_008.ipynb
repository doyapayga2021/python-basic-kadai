        "# 変数varの宣言\n",
        "var = 11\n",
        "# 変数varが3の倍数と5の倍数の場合「FizzBuzz」を出力\n",
        "if var%3 == 0 and var%5 == 0:\n",
        "    print(\"FizzBuzz\")\n",
        "# 変数varが3の倍数の場合「Fizz」を出力\n",
        "elif var%3 == 0:\n",
        "    print(\"Fizz\")\n",
        "# 変数varが5の倍数の場合「Buzz」を出力\n",
        "elif var%5 == 0:\n",
        "    print(\"Buzz\")\n",
        "# どの条件に当てはまらない場合varの数値を出力\n",
        "else:\n",
        "    print(var)"
