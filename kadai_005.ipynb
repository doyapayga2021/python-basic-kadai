        "# 台形面積計算ツール\n",
        "\n",
        "# 上辺設定\n",
        "uppder_side = 10\n",
        "# 下辺設定\n",
        "lower_side = 20\n",
        "# 高さ設定\n",
        "height = 5\n",
        "\n",
        "# 台形面積計算\n",
        "trapezoid_area = (uppder_side + lower_side) * height / 2\n",
        "\n",
        "# 台形面積出力\n",
        "print(f\"上辺10cm、下辺20cm、高さ5cmの台形面積は{trapezoid_area}㎠です。\")\n"
      ],
